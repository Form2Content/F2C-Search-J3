<?xml version="1.0" encoding="utf-8"?><extension type="module" version="3.0.0" client="site" method="upgrade">	<name>mod_form2contentsearch</name>	<creationDate>2020-05-21</creationDate>    <author>Open Source Design</author>    <authorEmail>info@opensourcedesign.nl</authorEmail>    <authorUrl>http://www.opensourcedesign.nl</authorUrl>    <copyright>(c) 2020 - Open Source Design</copyright>    <license>Commercial GPL (See http://www.form2content.com/faqs for more information)</license>	<version>6.7.1</version>	<description>MOD_FORM2CONTENTSEARCH_XML_DESCRIPTION</description>    <files>    	<filename module="mod_form2contentsearch">mod_form2contentsearch.php</filename>    	<filename>helper.php</filename>    	<filename>index.html</filename>		<filename>mod_form2contentsearch.xml</filename>		<folder>tmpl</folder>		<folder>fields</folder>	</files>	<languages>		<language tag="en-GB">language/en-GB/en-GB.mod_form2contentsearch.ini</language>		<language tag="en-GB">language/en-GB/en-GB.mod_form2contentsearch.sys.ini</language>				<language tag="nl-NL">language/nl-NL/nl-NL.mod_form2contentsearch.ini</language>		<language tag="nl-NL">language/nl-NL/nl-NL.mod_form2contentsearch.sys.ini</language>			</languages>	<media destination="mod_form2contentsearch" folder="media">			<filename>index.html</filename>			<folder>css</folder>			<folder>images</folder>			<folder>js</folder>	</media>		   	<config>   		<fields name="params">   			<fieldset name="basic" addfieldpath="/modules/mod_form2contentsearch/fields">   	   			   				   								<field name="moduleclass_sfx" type="text"					label="MOD_FORM2CONTENTSEARCH_MODULECLASS_SFX_LABEL"					description="MOD_FORM2CONTENTSEARCH_MODULECLASS_SFX_DESC" />   							<field name="id" type="sql"					label="MOD_FORM2CONTENTSEARCH_SEARCHFORM"					description="MOD_FORM2CONTENTSEARCH_SEARCHFORM_DESC"					query="SELECT id, title FROM #__f2c_search_form ORDER BY title" key_field="id" value_field="title" />				<field name="itemid" type="text"					label="MOD_FORM2CONTENTSEARCH_ITEMID"					description="MOD_FORM2CONTENTSEARCH_ITEMID_DESC" />				<field name="template" type="F2cSearchTemplate" hide_none="true" hide_default="true" default="default"					   label="MOD_FORM2CONTENTSEARCH_SELECT_TEMPLATE" description="MOD_FORM2CONTENTSEARCH_SELECT_TEMPLATE_DESC" 					   directory="modules/mod_form2contentsearch/tmpl" />				<field name="show_result_count" type="radio" default="1"					label="MOD_FORM2CONTENTSEARCH_SHOWRESULTCOUNT"					description="MOD_FORM2CONTENTSEARCH_SHOWRESULTCOUNT_DESC" class="btn-group">					<option value="1">MOD_FORM2CONTENTSEARCH_YES</option>					<option value="0">MOD_FORM2CONTENTSEARCH_NO</option>				</field>				<field name="show_result_count_start" type="radio" default="1"					label="MOD_FORM2CONTENTSEARCH_SHOWRESULTCOUNTSTART"					description="MOD_FORM2CONTENTSEARCH_SHOWRESULTCOUNTSTART_DESC" class="btn-group">					<option value="1">MOD_FORM2CONTENTSEARCH_YES</option>					<option value="0">MOD_FORM2CONTENTSEARCH_NO</option>				</field>				<field name="auto_search" type="radio" default="0"					label="MOD_FORM2CONTENTSEARCH_AUTOSEARCH"					description="MOD_FORM2CONTENTSEARCH_AUTOSEARCH_DESC" class="btn-group">					<option value="1">MOD_FORM2CONTENTSEARCH_YES</option>					<option value="0">MOD_FORM2CONTENTSEARCH_NO</option>				</field>																<field name="show_reset" type="radio" default="0"					label="MOD_FORM2CONTENTSEARCH_SHOWRESET"					description="MOD_FORM2CONTENTSEARCH_SHOWRESET_DESC" class="btn-group">					<option value="1">MOD_FORM2CONTENTSEARCH_YES</option>					<option value="0">MOD_FORM2CONTENTSEARCH_NO</option>				</field>																<field name="reset_refresh_results" type="radio" default="0"					label="MOD_FORM2CONTENTSEARCH_RESET_REFRESH_RESULTS"					description="MOD_FORM2CONTENTSEARCH_RESET_REFRESH_RESULTS_DESC" class="btn-group">					<option value="1">MOD_FORM2CONTENTSEARCH_YES</option>					<option value="0">MOD_FORM2CONTENTSEARCH_NO</option>				</field>															</fieldset>					</fields>	</config>	<updateservers>	   <server type="extension" priority="2" name="Form2content Search module">http://update.form2content.com/f2c_mod_f2csearch_j30.xml</server>	</updateservers>				</extension>